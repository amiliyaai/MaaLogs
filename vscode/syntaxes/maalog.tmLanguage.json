{
  "name": "MAA Log",
  "scopeName": "source.maalog",
  "patterns": [
    {
      "match": "^\\[([^\\]]+)\\]\\[([^\\]]+)\\]\\[([^\\]]+)\\]\\[([^\\]]+)\\]",
      "captures": {
        "1": { "name": "constant.numeric.date.maalog" },
        "2": {
          "name": "keyword.control.maalog",
          "patterns": [
            { "match": "DBG|TRC", "name": "comment.maalog" },
            { "match": "INF", "name": "entity.name.function.maalog" },
            { "match": "WRN", "name": "markup.changed.maalog" },
            { "match": "ERR", "name": "markup.deleted.maalog" }
          ]
        },
        "3": { "name": "variable.parameter.maalog" },
        "4": { "name": "variable.parameter.maalog" }
      }
    },
    {
      "match": "\\[([^\\]=]+)=([^\\]]+)\\]",
      "captures": {
        "1": { "name": "entity.name.tag.maalog" },
        "2": { "name": "string.unquoted.maalog" }
      }
    },
    {
      "match": "\\[([^\\]=]+)\\]",
      "captures": {
        "1": { "name": "constant.language.maalog" }
      }
    },
    {
      "match": "\\|\\s*(enter|leave)(?:,\\s*(\\d+)ms)?",
      "captures": {
        "1": { "name": "keyword.control.maalog" },
        "2": { "name": "constant.numeric.maalog" }
      }
    },
    {
      "match": "!!!OnEventNotify!!!",
      "name": "support.function.maalog"
    },
    {
      "match": "Tasker\\.Task\\.(Starting|Succeeded|Failed)",
      "name": "entity.name.function.maalog"
    },
    {
      "match": "Node\\.(Recognition|Action|NextList)\\.(Starting|Succeeded|Failed)",
      "name": "entity.name.function.maalog"
    },
    {
      "match": "MaaAdbControllerCreate|MaaWin32ControllerCreate",
      "name": "support.function.maalog"
    }
  ]
}
